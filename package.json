{
  "name": "spotify-wrapper",
  "version": "1.0.0",
  "description": "A wrapper to work with the Spotify Web API.",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint src/*.js",
    "prepush": "npm run lint && npm run test:coverage",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register",
    "test:tdd": "yarn test -- --watch",
    "test:coverage": "nyc npm test"
  },
  "nyc": {
    "functions": 80,
    "lines": 80,
    "check-coverage": true,
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wnqueiroz/js-tdd-course.git"
  },
  "keywords": [
    "js",
    "tdd",
    "library"
  ],
  "author": "William Queiroz <williamdenovaisqueiroz@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wnqueiroz/js-tdd-course/issues"
  },
  "homepage": "https://github.com/wnqueiroz/js-tdd-course#readme",
  "devDependencies": {
    "babel-preset-env": "1.7.0",
    "babel-register": "6.26.0",
    "chai": "3.5.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "13.0.0",
    "eslint-plugin-import": "2.13.0",
    "husky": "1.0.0-rc.9",
    "mocha": "5.2.0",
    "node-fetch": "^2.2.0",
    "nyc": "13.0.1",
    "sinon": "^6.2.0",
    "sinon-chai": "^3.2.0"
  }
}
